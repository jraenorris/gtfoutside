  <div class="row">
    <div class="small-4 large-4 columns"></div>
  </div>
  You're in <%= @city %>, <%= @region %> -
  <div>
    <iframe
      width="600"
      height="450"
      frameborder="0" style="border:0"
      src="https://www.google.com/maps/embed/v1/place?key=#{ENV['GOOGLE_KEY']}
        &q=<%= @city %>, <%= @region %>
        &attribution_source=Google+Maps+Embed+API
        &attribution_ios_deep_link_id=comgooglemaps://?daddr=<%= @city %>, <%= @region %>">
    </iframe>
  </div>

  <div class="main-list">
    <ol>
      <% @places.each do |place| %>
        <li>
          <%= link_to "#{place['name']}", park_path(place['place_id']) %>
          <ul><li>Rating: <%= place["rating"] %></li></ul>
        </li>
      <% end %>
    </ol>
  </div>
